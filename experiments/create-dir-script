# Create Directory Structure

for dname in AlgorithmTesting BBS CCG G-SHA1 LCG MODEXP MS QCG1 QCG2 XOR ; do
  for test in Frequency BlockFrequency Runs LongestRun Rank FFT \
	    NonOverlappingTemplate OverlappingTemplate Universal LinearComplexity \
			Serial ApproximateEntropy CumulativeSums RandomExcursions \
			RandomExcursionsVariant Gambler; do
		mkdir -p $dname/$test
	done
done
